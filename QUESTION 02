#Quiz 10 - Question 2

def dotprod(x,y):
    T = 0
    if (len(x) == len(y)):
        for i in range(len(x)):
            T = T + x[i]* y[i] 
        return T
    else:
        return ("ERROR!!!")
            
n1 = [2,4,5,6]
n2 = [1,2,3,4]
print()
print(n1)
print(n2)
print()
print(dotprod(n1,n2))
